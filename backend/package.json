{
  "name": "contadash-backend",
  "version": "1.0.0",
  "description": "ContaDash API Backend",
  "main": "dist/server.js",
  "scripts": {
    "dev": "nodemon --exec tsx src/server.ts",
    "build": "prisma generate && (tsc || exit 0) && cp -r src/templates dist/ && cp -r src/scripts dist/",
    "start": "prisma migrate deploy && node dist/scripts/fix-recurring-dates-utc.js && node dist/scripts/load-january-2025-income.js && node dist/scripts/load-february-2025-income.js && node dist/server.js",
    "postinstall": "prisma generate",
    "migrate:deploy": "prisma migrate deploy",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:seed": "tsx prisma/seeds/seed.ts",
    "prisma:studio": "prisma studio",
    "cleanup:duplicates": "tsx scripts/cleanup-duplicate-credit-cards.ts",
    "import:data": "tsx src/scripts/import-production-data.ts",
    "fix:recurring": "tsx src/scripts/fix-recurring-transactions.ts",
    "fix:manual": "tsx src/scripts/manual-fix-user-transactions.ts",
    "create:recurring-jan2026": "tsx src/scripts/create-recurring-transactions-jan2026.ts"
  },
  "keywords": [
    "api",
    "express",
    "typescript"
  ],
  "author": "ContaDash Team",
  "license": "UNLICENSED",
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "@sendgrid/mail": "^8.1.6",
    "axios": "^1.13.2",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "exceljs": "^4.4.0",
    "express": "^4.18.2",
    "express-rate-limit": "^7.5.1",
    "helmet": "^7.2.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.2",
    "node-cron": "^4.2.1",
    "nodemailer": "^7.0.11",
    "puppeteer": "^24.31.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/multer": "^2.0.0",
    "@types/node": "^20.10.6",
    "@types/node-cron": "^3.0.11",
    "@types/nodemailer": "^7.0.4",
    "nodemon": "^3.0.2",
    "prisma": "^5.22.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vitest": "^1.1.0"
  },
  "prisma": {
    "seed": "tsx prisma/seeds/seed.ts"
  }
}
